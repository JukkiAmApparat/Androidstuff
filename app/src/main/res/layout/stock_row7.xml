<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_gravity="fill"
        android:lines="2"
        android:id="@+id/stock7_name_cell"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_last_cell"
        android:gravity="right"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_low_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_plus_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_time_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_plus3m_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_plus6m_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
    <TextView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/stock7_plus1y_cell"
        android:gravity="left"
        android:layout_gravity="fill"
        android:lines="2"
        android:textStyle="normal"/>
</TableRow>